/user/baoliaos:
  get:
    summary: 获取用户爆料
    description: |
      获取用户爆料列表
    parameters:
      - $ref: '#/parameters/appid'
      - $ref: '#/parameters/signature'
      - $ref: '#/parameters/timestamp'
      - $ref: '#/parameters/client_id'
      - $ref: '#/parameters/token'
      - $ref: '#/parameters/page'
      - $ref: '#/parameters/per_page'
    tags:
      - User
    responses:
      '200':
        description: 获取用户爆料列表，带回复
        schema:
          type: object
          properties:
            code:
              $ref: '#/definitions/C'
            msg:
              $ref: '#/definitions/M'
            data:
              type: array
              items:
                $ref: '#/definitions/BaoliaoOne'
      '403':
        description: 无法通过token获取数据
        schema:
          $ref: '#/definitions/Forbidden'