}
.select2-results .select2-selected{
  display:none!important;
}
.select2-results li{
  line-height:24px;
}

/* side category */
.page-sidebar .page-sidebar-menu .sub-menu li > .sub-menu > li > .sub-menu > li > .sub-menu > li > .sub-menu > li > a, 
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu .sub-menu li > .sub-menu > li > .sub-menu > li > .sub-menu > li > .sub-menu > li > a{
