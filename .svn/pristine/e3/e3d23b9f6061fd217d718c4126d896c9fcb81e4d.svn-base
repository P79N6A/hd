/* kuangmail 2015-9-1 */

/* public */
html, body{
  font-family:"Microsoft YaHei", Arial, sans-serif;
  margin:0;
  padding:0;
}
body{
  background:#fff;
}
h1, h2, h3, h4, h5, h6, .label, .datetimepicker table td, .datetimepicker table th, .datepicker table th, .datepicker table td{
  font-family:"Microsoft YaHei", Arial, sans-serif!important;
}
.f-right{
  float:right;
}
.imgbox{
  padding:2px; 
  border:1px solid #ddd;
}

/* iframe */
.iframe-main{
  overflow:hidden;
  position:relative;
}
.iframe-main .page-container{
  margin-top:0;
}

/* page header */
.page-header.navbar .page-logo{
  padding-left:10px;
  padding-right:10px;
}
.page-header.navbar .page-logo a{
  color:#fff;
  font-weight:700;
  font-size:18px;
  line-height:46px;
}
.page-header.navbar .page-logo a:hover{
  text-decoration:none;
}
.page-header.navbar .top-menu .navbar-nav{
  margin-right:5px;
}
.page-header.navbar .page-logo .logo-default{
  height:35px;
  margin-top:-3px;
}
.page-header.navbar .top-menu .navbar-nav > li.dropdown > .dropdown-toggle:last-child{
  padding:0 5px;
  line-height:46px;
  height:46px;
}
.page-header.navbar .top-menu .navbar-nav > li.dropdown-user .dropdown-toggle > img {
  float:left;
  margin-top:9px;
  margin-right:6px;
  height:29px;
  width:29px;
  display:inline-block;
}
.search-result {
  padding:10px 15px;
  border-top:1px solid #e5e5e5;
}
.search-result dl {
  margin-top:3px;
  margin-bottom:3px;
}
.search-result dt{
  display:inline-block;
}
.search-result dd{
  padding:2px 6px;
  border:1px solid #ccc;
  cursor:pointer;
  background-color:#eee;
  display:inline-block;
  margin-right:5px;
}
.search-result dd:hover{
  border:1px solid #d64635;
}
.search-result dd span{
  color:#d64635;
}
.search-result dd a{
  margin:5px 0 0 10px;
  color:#d64635;
}
.page-header.navbar.navbar-static-top{
  z-index:998;
}

/* page sidebar */
.page-sidebar-fixed .page-sidebar{
  z-index:10;
}

.page-sidebar-closed .page-bar-fixed .page-breadcrumb, .page-sidebar-closed .page-bar-fixed .screening-content{
  margin-left:45px;
}
.page-sidebar{
  position:relative;
  z-index:101;
}
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu > li:hover > a, .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu > li.open > a,
.page-sidebar .page-sidebar-menu > li.open > a{
  background: rgb(187, 127, 0);
  border-top-color: transparent;
  color: #ffffff;
}
.page-sidebar .page-sidebar-menu > li.open > a > .selected{
  display: block;
  background-image: none;
  /* will be set in a theme css file*/
  float: right;
  position: absolute;
  right: 0px;
  top: 8px;
  background: none;
  width: 0;
  height: 0;
  border-top: 12px solid transparent;
  border-bottom: 12px solid transparent;
  border-right: 12px solid #ffffff;
}

/* page content */
.page-main{
  overflow-x:hidden;
}
.page-content-wrapper .page-content{
  padding:1px;
  min-height:inherit!important;
}
.page-bar{
  /*margin-bottom:10px;*/
  background:none;
}
.page-bar .page-breadcrumb{
  float:none;
  display:block;
  padding:0 0 0 10px;
  background-color:#eee;
}
.page-bar .page-breadcrumb > li{
  line-height:36px;
  height:36px;
}
.page-bar .page-breadcrumb > li.active a{
  color:#3598dc;
}
.page-bar .page-breadcrumb > li.page-toolbar{
  line-height:inherit;
}
.page-bar .page-breadcrumb li .label-sm{
  color:#fff;
  margin:0 5px;
}
.page-bar .page-toolbar .btn{
  padding:8px 14px;
}
.page-bar .page-toolbar .form-inline{
  float:left;
  margin-right:10px;
}
.page-bar .page-toolbar .form-inline .form-control{
  height:36px;
}

.page-header.navbar .top-menu .navbar-nav > li.dropdown .dropdown-toggle > i.icon-logout{
  transform:rotate(180deg);
}
.page-bar-fixed{
  position:fixed;
  left:0; 
  top:0;
  width:100%;
  margin:0;
  z-index:100; 
}
.breadcrumb-choose{
  display:inline-block;
  border:1px solid #999;
  padding:1px 20px 1px 5px;
  margin-bottom:0;
  line-height:18px;
  position:relative;
  cursor:pointer;
}
.breadcrumb-choose button.close{
  position:absolute;
  top:5px;
  right:5px;
}
.breadcrumb-choose a:hover{
  text-decoration:none;
}
.breadcrumb-choose:hover{
  border:1px solid #0f4e74;
}
.breadcrumbbtn{
  width:65px;
}
.fullscreen-mode{
  height:100%!important;
  left:0;
  position:fixed;
  top:0;
  width:100%;
  margin:0!important;
  z-index:999;
  background-color:#fff;
}
.fullscreen-mode .paginationnav .paginationnav-main, .fullscreen-mode .debug-info table{
  margin-left:0;
}
.page-sidebar-closed .page-content-wrapper .fullscreen-mode{
  margin-left:0!important;
}
.screening-content{
  background-color:#fff;
  border:1px solid #eee;
  border-top:none;
}
.screening-content .form .form-bordered .form-group > div{
  padding:10px 15px;
}
.screening-content .form .form-bordered .form-group .control-label{
  padding-top:17px;
  text-align:right;
  max-width:100px!important;
}
.screening-box{
  display:none;
}
.screening-btn{
  display:none;
}
.order-filter{
  padding:6px 0;
}
.order-filter a{
  color:#555;
  padding:2px 10px;
  margin-right:2px;
  margin-bottom:5px;
  background:#eee;
  display:inline-block;
}
.order-filter a.active, .order-filter a:hover{
  color: #fff;
  background: #0da3e2;
  text-decoration:none;
}
.linkage-select{
  overflow:hidden;
  margin-bottom:10px;
}
div.tagsinput span.tag{
  background:#2386ca;
}
div.tagsinput input, .select2-container-multi .select2-choices .select2-search-field input{
  font-family:"Microsoft YaHei", Arial, sans-serif!important;
}
.screening-order input, .screening-order select{
  display:inline-block;
  vertical-align:top;
}
/* upload */
.file-footer-buttons .btn-default > i.text-info {
    color: #89C4F4;
}
.file-footer-buttons .btn-default > i.text-danger {
    color: #F3565D;
}

/* category */
.level-1, .level-2, .level-3, .level-4, .level-5, .level-6, .level-7, .level-8, .level-9, .level-10, .level-11, .level-12{
  line-height:24px;
  position:relative;
}
.level-1 .select2-result-label{
