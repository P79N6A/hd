}
.level-2 .select2-result-label{
  margin-left:20px;
}
.level-3 .select2-result-label{
  margin-left:40px;
}
.level-4 .select2-result-label{
  margin-left:60px;
}
.level-5 .select2-result-label{
  margin-left:80px;
}
.level-6 .select2-result-label{
  margin-left:100px;
}
.level-7 .select2-result-label{
  margin-left:120px;
}
.level-end{
  display:inline-block!important;
}
.level-end .select2-result-label{
  padding-left:10px;
  padding-right:10px;
}
.select2-highlighted .select2-result-label{
  background-color:#3598dc; 
  color:#fff;
}
.level-end.select2-highlighted{
