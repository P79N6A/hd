/* Custom vertical inline menu */
.anchor-orientation {
    position:fixed;
    top:100px;
    right:-120px;
    z-index:10;
    width:160px;
}
.anchor-popup-effect {
    position:fixed;
    right:120px;
}
.anchor-inline-menu {
    padding:0;
    margin:0;
    list-style:none;
}
.anchor-inline-menu li {
    position:relative;
    margin-bottom:2px;
}
.anchor-inline-menu li i {
    width:40px;
    height:40px;
    display:inline-block;
    color:#b9cbd5;
    font-size:15px;
    padding:12px 10px 10px 8px;
    margin:0 8px 0 0;
    text-align:center;
    background:#e0eaf0 !important;
}
.anchor-inline-menu li a {
    font-size:14px;
    font-weight:300;
    color:#557386;
    display:block;
    background:#f0f6fa;
    border-left:solid 2px #c4d5df;
}
.anchor-inline-menu li:hover a {
    background:#e0eaf0;
    text-decoration:none;
}
.anchor-inline-menu li:hover i {
    color:#fff;
    background:#c4d5df !important;
}
.anchor-inline-menu li.active a {
    border-left:solid 2px #0c91e5;
}
.anchor-inline-menu li.active i {
    background:#0c91e5 !important;
}
.anchor-inline-menu li.active a, .anchor-inline-menu li.active i {
