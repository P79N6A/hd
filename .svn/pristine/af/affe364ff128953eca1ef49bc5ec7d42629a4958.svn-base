/me/signature:
  post:
    summary: 修改用户签名
    parameters:
      - $ref: '#/parameters/appid'
      - $ref: '#/parameters/signature'
      - $ref: '#/parameters/timestamp'
      - $ref: '#/parameters/client_id'
      - $ref: '#/parameters/token'
      - name: signature
        in: formData
        description: 应用裁剪过的方形图片
        required: true
        type: string
    tags:
      - User
    responses:
      '200':
        description: 处理成功
        schema:
          type: object
          properties:
            code:
              $ref: '#/definitions/C'
            msg:
              $ref: '#/definitions/M'
      '403':
        description: 无法通过token获取用户信息
        schema:
          $ref: '#/definitions/Forbidden'