.layer .share{position:absolute; top:50px; left:38%; margin-left:40%; width:82px; height:45px; background:url(../images/share.png?ver18) no-repeat; background-size:100%; z-index:100; cursor:pointer;}
.layer_yes{position:absolute; top:0; left:0; z-index:10; width:100%; height:100%; background-color:#1b163a; display:none;}
.text_con_info{width:89%; position:relative; margin:auto; background:#2c0f61; border-radius:0 0 0.5rem 0.5rem; z-index:25; color:#fff; font-size:2rem; padding-top:1rem;}
.text_con_info .input_l{padding:1rem 2rem;}
.text_con_info .input_l input{width:100%; text-indent:10px; height:56px; line-height:56px; background:#f9f9f9; padding:0; border:none; color:#333; font-size:2rem}
.text_con_info .input_l #code{float:left; width:40%;}
.text_con_info .input_l #getCode{float:right; text-decoration:none; width:50%; text-align:center; height:56px; line-height:56px; background:#eb3c3a; color:#fae001;}
.text_con_info .input_l #getCode.disabled{background:#999; cursor:not-allowed;}
.text_con_info .input_l #submitTel{text-align:center; text-decoration:none; display:block; height:4rem; line-height:4rem; background:#ed3c3a; border:none; color:#fae001; font-size:2.4rem; font-weight:bold;}
.text_con_info .input_l textarea{width:100%; height:156px; font-size:2rem; text-indent:10px; padding:5px 0;}
.text_con_info p.rule-info{text-align:left; line-height:2.8rem; padding:1rem 2rem; font-size:1.8rem;}
.prize-name{color:#040300; position:absolute; top:205px; font-size:3rem; width:80%; left:50%; margin-left:-40%; text-align:center;}
#city_china_val select{width:32%; margin-right:2%; float:left; height:56px; line-height:56px; font-size:2rem;}
#city_china_val select:last-child{margin-right:0; float:right;}
.share_con{position:absolute; top:0; left:0; z-index:10; width:100%; height:100%; background:rgba(0,0,0,.3); display:none;}
.share_main{background:#fff; position:fixed; bottom:0; left:0; width:100%;}
.share-list{text-align:center; padding:2rem; border-bottom:1px solid #666;}
.share-list a{display:inline-block;  margin:0 5rem;}
.share-list a img{width:100%;}
.share_main .share-close{display:block; height:100px; line-height:100px; text-align:center; font-size:3rem; color:#333; letter-spacing:10px; text-decoration:none;}
#showInput{width:240px; height:60px; font-size:2rem; display:block; margin:1rem auto; border:none;  border-radius:5px;}
.go-luck{display:block;}
.yao_btn_animation{
    -webkit-animation: 500ms shake-hard-animation infinite;
    -ms-animation: 500ms shake-hard-animation infinite;
    animation: 500ms shake-hard-animation infinite;
    -webkit-animation-timing-function: ease-in-out;
    -ms-animation-timing-function: ease-in-out;
    animation-timing-function: ease-in-out;
    -webkit-animation-delay: 0s;
    -ms-animation-delay: 0s;
    animation-delay: 0s;
    -webkit-animation-play-state: running;
    -ms-animation-play-state: running;
    animation-play-state: running
}
@-webkit-keyframes shake-hard-animation{
    0% {
        -webkit-transform: rotate(20deg);
           -moz-transform: rotate(20deg);
            -ms-transform: rotate(20deg);
             -o-transform: rotate(20deg);
                transform: rotate(20deg); }
    
    50% {
        -webkit-transform: rotate(0deg);
           -moz-transform: rotate(0deg);
            -ms-transform: rotate(0deg);
             -o-transform: rotate(0deg);
                transform: rotate(0deg); }
    100% {
        -webkit-transform: rotate(20deg);
           -moz-transform: rotate(20deg);
            -ms-transform: rotate(20deg);
             -o-transform: rotate(20deg);
                transform: rotate(20deg); }
}
.yao_btn_dong{
    -webkit-animation: 100ms shake-hard infinite;
    -ms-animation: 100ms shake-hard infinite;
    animation: 100ms shake-hard infinite;
    -webkit-animation-timing-function: ease-in-out;
    -ms-animation-timing-function: ease-in-out;
    animation-timing-function: ease-in-out;
    -webkit-animation-delay: 0s;
    -ms-animation-delay: 0s;
    animation-delay: 0s;
    -webkit-animation-play-state: running;
    -ms-animation-play-state: running;
    animation-play-state: running
}

@-webkit-keyframes shake-hard{
    0% {
        -webkit-transform: translate(0px, 0px) rotate(0deg)
    }
    20% {
        -webkit-transform: translate(-8px, -8px) rotate(0.5deg)
    }
    40% {
        -webkit-transform: translate(-7px, 1px) rotate(-3.5deg)
    }
    60% {
        -webkit-transform: translate(-3px, 2px) rotate(-0.5deg)
    }
    80% {
        -webkit-transform: translate(-5px, 3px) rotate(2.5deg)
    }
    98% {
        -webkit-transform: translate(-5px, 6px) rotate(0.5deg)
    }
}
