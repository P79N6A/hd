/votes/{id}:
  post:
    summary: 提交投票
    tags:
      - Votes
    description: 提交投票
    parameters:
      - name: id
        in: path
        description: 投票项目ID|此字段`不参与`授权加密运算
        required: true
        type: integer
      - name: votes_key
        in: formData
        description: 投票选项ID
        required: true
        type: integer
      - $ref: '#/parameters/Token'
      - $ref: '#/parameters/appid'
      - $ref: '#/parameters/signature'
      - $ref: '#/parameters/timestamp'
      - $ref: '#/parameters/client_id'
    responses:
      '200':
        description: 成功结果
        schema:
          type: object
          properties:
            code:
              $ref: '#/definitions/C'
            msg:
              $ref: '#/definitions/M'
      '404':
        description: 出错返回
        schema:
          $ref: '#/definitions/NotFound'